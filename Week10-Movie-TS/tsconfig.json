{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2020",                
    "module": "ESNext",              
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}